        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": false,
        "outDir": "dist",
        "declaration": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "typings/globals",
        "typings/modules",
        "dist"
    ]
}